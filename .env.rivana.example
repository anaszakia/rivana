# ============================================
# RIVANA HIDROLOGI ML API CONFIGURATION
# ============================================

# API URL - Ganti dengan URL VPS Anda
RIVANA_API_URL=http://103.127.136.132:8000

# API Token - Harus sama dengan token di Python API (.env.production)
# Generate dengan: python -c "import secrets; print(secrets.token_urlsafe(32))"
RIVANA_API_TOKEN=your_secure_token_here

# Request Timeout (detik) - untuk proses yang lama
RIVANA_API_TIMEOUT=300

# Enable/Disable API Logging
RIVANA_API_LOGGING=true

# Retry Configuration
RIVANA_API_RETRY=3
RIVANA_API_RETRY_SLEEP=100
